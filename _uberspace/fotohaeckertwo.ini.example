[program:fotohaeckertwo]
command=mix phx.server
directory=DIRECTORY
environment=
    MIX_ENV="prod",
    PORT=PORT,
    DATABASE_PATH=DATABASE_PATH,
    SECRET_KEY_BASE=SECRET_KEY_BASE,
    PHX_HOST=PHX_HOST,
    AUTH0_CLIENT_ID=AUTH0_CLIENT_ID,
    AUTH0_CLIENT_SECRET=AUTH0_CLIENT_SECRET,
    AUTH0_DOMAIN=AUTH0_DOMAIN,
    AUTH0_MANAGEMENT_CLIENT_ID="AUTH0_MANAGEMENT_CLIENT_ID",
    AUTH0_MANAGEMENT_CLIENT_SECRET="AUTH0_MANAGEMENT_CLIENT_SECRET",
    # optional
    # CLARIFAI_API_SECRET="CLARIFAI_API_SECRET",
    # STRIPE_SECRET="STRIPE_SECRET",
    # STRIPE_CONNECT_CLIENT_ID="STRIPE_CONNECT_CLIENT_ID",
    # STRIPE_PRICE_ID="STRIPE_PRICE_ID"
autostart=yes
autorestart=yes