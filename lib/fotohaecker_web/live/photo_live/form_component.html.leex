<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "photo-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :title %>
  <%= text_input f, :title %>
  <%= error_tag f, :title %>

  <%= label f, :description %>
  <%= text_input f, :description %>
  <%= error_tag f, :description %>

  <%= label f, :path %>
  <%= text_input f, :path %>
  <%= error_tag f, :path %>

  <%= label f, :tags %>
  <%= multiple_select f, :tags, ["Option 1": "option1", "Option 2": "option2"] %>
  <%= error_tag f, :tags %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
